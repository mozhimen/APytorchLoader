<?xml version="1.0" encoding="utf-8"?>
<!--设置形状为椭圆形-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!--颜色设置为黑色-->
    <stroke
        android:color="@color/colorBlack"
        android:width="1dip"/>
    <solid android:color="#F0FFFF"/>
    <!--设置长轴和短轴分别为80dp-->
    <size android:width="80dp" android:height="80dp"/>
</shape>